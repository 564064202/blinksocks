{
  "name": "blinksocks",
  "version": "2.5.1",
  "description": "A framework for building composable proxy protocol stack",
  "main": "build/index.js",
  "files": [
    "bin",
    "build",
    "AUTHORS"
  ],
  "bin": {
    "blinksocks": "bin/cli.js",
    "blinksocks-init": "bin/cli-init.js",
    "blinksocks-client": "bin/cli-client.js",
    "blinksocks-server": "bin/cli-server.js"
  },
  "scripts": {
    "test": "npm run lint && npm run test:coverage",
    "test:coverage": "jest --coverage",
    "lint": "eslint bin src pkg",
    "compile": "cross-env NODE_ENV=production babel src --out-dir build --minified --ignore __tests__,__mocks__",
    "bundle": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "prepack": "rimraf build/blinksocks.js.map pkg/blinksocks-* pkg/sha256sum.txt",
    "pack": "pkg --out-path pkg/ .",
    "postpack": "node pkg/postpkg.js",
    "debug:client": "cross-env NODE_ENV=development node --inspect --inspect-port=9229 bin/cli-client.js --config blinksocks.client.json",
    "debug:server": "cross-env NODE_ENV=development node --inspect --inspect-port=9230 bin/cli-server.js --config blinksocks.server.json",
    "client": "node bin/cli-client.js --config blinksocks.client.json",
    "server": "node bin/cli-server.js --config blinksocks.server.json",
    "precommit": "npm test"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "ip": "^1.1.5",
    "urijs": "^1.18.10",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^5.0.4",
    "eslint": "^4.3.0",
    "eslint-config-babel": "^7.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "pkg": "^4.2.2",
    "rimraf": "^2.6.1",
    "webpack": "^3.4.1"
  },
  "repository": {
    "url": "https://github.com/blinksocks/blinksocks",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/blinksocks/blinksocks/issues"
  },
  "homepage": "https://github.com/blinksocks/blinksocks",
  "keywords": [
    "socks5",
    "socks4",
    "socks4a",
    "http",
    "protocol",
    "proxy",
    "nodejs",
    "node",
    "blinksocks",
    "shadowsocks",
    "shadowsocksr",
    "middleware",
    "flexible",
    "composable",
    "customizable",
    "aead",
    "obfs"
  ],
  "author": "Micooz <micooz@hotmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 6"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
